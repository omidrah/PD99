@model  Web.Areas.Admin.Controllers.prId

@{
    ViewBag.Title = "جستجو " + Html.DisplayNameFor(m => m);
}
<section>
    <div class="row text-center"> <h3>چاپ آدرس براساس کالای جدید <b>@ViewBag.PackageTitle</b></h3></div>
    <div class="row text-center"> <h4>انتخاب کالا @*<b>@ViewBag.PackageTitle</b>*@</h4></div>

    <hr />
    @using (Html.BeginForm("GetAllOrderDetialByProductIdNotTransfered", "OrderDetail", FormMethod.Post))
    {
        @Html.AntiForgeryToken()

        <div class="form-horizontal">
            @Html.ValidationSummary(true)

            <div class="form-group">
                @Html.Label("عنوان کالا", new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.DropDownList("ProductId", null, "انتخاب کنید", new { @class = "form-control" })
                    @Html.ValidationMessageFor(model => model.ProductId)
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="جستجو" class="btn btn-success" />
                    @Html.ActionLink("بازگشت", "Index","Orders",null, new { @class = "btn btn-primary" })
                </div>
            </div>
        </div>
    }

</section>
@section scripts{
    @Html.Raw(TempData["res"])
}